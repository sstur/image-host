{
  "name": "image-host",
  "version": "0.0.0",
  "scripts": {
    "start": "ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
    "start:prod": "node lib/index.js",
    "build": "tsc --outDir lib/",
    "clean": "rm -rf lib",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "test": "npm run typecheck && npm run format:check"
  },
  "devDependencies": {
    "@types/node": "^20.1.4",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "private": true
}
